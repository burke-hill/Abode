<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1F4A59"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="1654dp"
        android:layout_height="1095dp"
        app:layout_constraintBaseline_toBaselineOf="parent"
        app:srcCompat="@drawable/apartment_building_architectural_details_modern_39732665"
        tools:layout_editor_absoluteX="-260dp" />

    <Button
        android:id="@+id/availabilityButton"
        android:layout_width="204dp"
        android:layout_height="108dp"
        android:layout_marginStart="88dp"
        android:layout_marginBottom="110dp"
        android:backgroundTint="#CC802B"
        android:text="Availability"
        android:onClick="availabilityLink"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:layout_constraintVertical_bias="0.987" />

    <Button
        android:id="@+id/button9"
        android:layout_width="204dp"
        android:layout_height="108dp"
        android:layout_marginStart="400dp"
        android:layout_marginBottom="110dp"
        android:backgroundTint="#CC802B"
        android:text="Gallery"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button"
        android:layout_width="204dp"
        android:layout_height="108dp"
        android:layout_marginStart="720dp"
        android:layout_marginBottom="110dp"
        android:backgroundTint="#CC802B"
        android:text="Amenities"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button10"
        android:layout_width="204dp"
        android:layout_height="108dp"
        android:layout_marginStart="984dp"
        android:layout_marginBottom="110dp"
        android:backgroundTint="#CC802B"
        android:text="Neighborhood"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="142dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/thehenry_logo" />





</androidx.constraintlayout.widget.ConstraintLayout>